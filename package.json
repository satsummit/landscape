    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.2",
    "gulp-useref": "^3.0.5",
    "gulp-util": "^3.0.7",
    "jeet": "^6.1.2",
    "node-bourbon": "^4.2.3",
    "node-notifier": "^4.5.0",
    "node-sass": "^3.7.0",
    "require-globify": "^1.3.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
