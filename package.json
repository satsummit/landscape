{
  "name": "landscape",
  "version": "2.0.0",
  "description": "Satellite State of Play - a strategy document on satellite imagery.",
  "repository": {
    "type": "git",
    "url": "https://github.com/satsummit/landscape.git"
  },
  "author": {
    "name": "Development Seed",
    "url": "https://developmentseed.org"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/satsummit/landscape/issues"
  },
  "homepage": "http://landscape.satsummit.io/",
  "scripts": {
    "install": "bundle install --path bundle/cache",
    "postinstall": "[ -f _config-dev.yml ] || touch _config-dev.yml",
    "serve": "gulp collecticons && gulp serve",
    "build": "DS_ENV=production NODE_ENV=production gulp",
